        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Smash_Character_Database_Editor"
        mc:Ignorable="d"
        Title="Preview CSS" Height="224" Width="920" ResizeMode="CanMinimize" Closed="Window_Closed">
    <Grid x:Name="RootGrid" HorizontalAlignment="Left" VerticalAlignment="Top" Width="900" Margin="0,0,0,0" Height="195">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
